{
  "arrowParens": "always",
  "endOfLine": "auto",
  "tabWidth": 2,
  "jsxSingleQuote": true,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "printWidth": 120,
  "useTabs": false,
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindAttributes": ["className", "class"],
  "tailwindFunctions": ["cn", "clsx", "twMerge"]
}